            <div class="mb-3">
                <label for="numPlayers" class="form-label">Number of Players</label>
                <input type="number" name="number" class="form-control" id="numPlayers" placeholder="Enter number of players" required>
            </div>
            
            <div class="mb-3">
                <label for="overs" class="form-label">Overs</label>
                <input type="number" name="overs" class="form-control" id="overs" placeholder="Enter number of overs" required>
            </div>
            
            <div class="mb-3">
                <label for="tossWonBy" class="form-label">Toss won by</label>
                <input type="text" name="toss" class="form-control" id="tossWonBy" placeholder="Enter team name" required>
            </div>
