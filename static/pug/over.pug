<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Gully Cricket</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
    <div class="container">
        <h1 class="text-center">Gully Cricket</h1>
        <div class="row">
            <div class="col-12">
                <h2 class="text-center">Game Over</h2>
                <h3 class="text-center">Team1 Score: <span id="team1"></span></h3>
                <h3 class="text-center">Team2 Score: <span id="team2"></span></h3>
                <div class="text-center">
                    <a href="/play" class="btn btn-primary">Play Again</a>
                </div>
            </div>
        </div>
    </div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
script.
    const team1S = document.getElementById('team1');
    const team2S = document.getElementById('team2');
    const team1 = JSON.parse(localStorage.getItem('team1Score'));
    const team2 = JSON.parse(localStorage.getItem('team2Score'));
    team1S.innerText = team1[0];
    team2S.innerText = team2[0];

</body>
</html>